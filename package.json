{
  "name": "trattoria-fedeli",
  "version": "1.0.0",
  "description": "Back-end application made for Trattoria Fedeli, a fictious italian restaurant made for the first challenge of OneBitCode.",
  "main": "./src/app.ts",
  "scripts": {
    "cdb": "sequelize-cli db:create",
    "ddb": "sequelize-cli db:drop",
    "mas": "npm run mi && npm run sd",
    "create": "npm run cdb && npm run mas && npm run dev",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/app.ts",
    "mg": "sequelize-cli migration:generate --name",
    "sg": "sequelize-cli seed:generate --name",
    "mi": "sequelize-cli db:migrate",
    "mu": "sequelize-cli db:migrate:undo",
    "sd": "sequelize-cli db:seed:all",
    "format": "prettier --write src/",
    "test": "jest --watchAll"
  },
  "keywords": [
    "adminjs",
    "api",
    "boilerplate",
    "boilerplate-code",
    "express",
    "jest",
    "sequelize",
    "typescript"
  ],
  "author": "daviebatista",
  "license": "ISC",
  "dependencies": {
    "@adminjs/express": "^5.1.0",
    "@adminjs/sequelize": "^3.0.0",
    "@adminjs/upload": "^3.0.1",
    "@tiptap/pm": "^2.1.13",
    "adminjs": "^6.8.7",
    "bcrypt": "~5.0.1",
    "connect-session-sequelize": "^7.1.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "env-var": "^7.3.1",
    "express": "^4.18.2",
    "express-formidable": "~1.2.0",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "pg": "~8.7.1",
    "sequelize": "^6.31.0"
  },
  "devDependencies": {
    "@types/bcrypt": "~5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "~4.17.13",
    "@types/express-session": "^1.17.7",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "~17.0.10",
    "@types/supertest": "^2.0.12",
    "prettier": "3.0.2",
    "sequelize-cli": "~6.4.1",
    "ts-node-dev": "~1.1.8",
    "typescript": "~4.5.4"
  }
}
